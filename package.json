{
  "name": "jewely",
  "private": true,
  "dependencies": {
    "-": "0.0.1",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@rails/webpacker": "^4.3.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.2.2",
    "g": "^2.0.1",
    "jquery": "^3.5.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.2",
    "react-responsive-carousel": "^3.2.11",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.8"
  },
  "description": "To run, run `npm i && npm start` to start frontend. `bundle install && rails s` to start backend.",
  "version": "1.0.0",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack --watch --mode=development",
    "postinstall": "webpack"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-console": "warn",
      "import/first": "error"
    }
  },
  "prettier": {
    "printWidth": 80,
    "bracketSpacing": false,
    "trailingComma": "es5",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ],
    "{app,test}/**/*.rb": [
      "rubocop -A"
    ]
  },
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hakeemmidan/Jewely.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hakeemmidan/Jewely/issues"
  },
  "homepage": "https://github.com/Hakeemmidan/Jewely#readme",
  "devDependencies": {
    "css-loader": "^3.2.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "2.1.1",
    "style-loader": "^1.3.0",
    "webpack-manifest-plugin": "^3.0.0"
  }
}
